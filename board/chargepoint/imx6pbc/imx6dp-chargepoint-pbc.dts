/* disable dvfs as it appears to be causing linux hang after boot */
&busfreq {
	status = "disabled";
};

&can1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1>;
	status = "okay";
};

&can2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan2>;
	status = "okay";
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet>;
	phy-mode = "rgmii";
	status = "okay";

	fixed-link {
		speed = <1000>;
		full-duplex;
		pause;
		asym-pause;
	};

	mdio1: mdio {
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		switch0: switch0@0 {
			compatible = "marvell,mv88e6085";
			reg = <0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_enet_switch>;
			reset-gpios = <&gpio1 3 GPIO_ACTIVE_LOW>;

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					label = "cpu";
					ethernet = <&fec>;
					phy-mode = "rgmii-id";

					fixed-link {
						speed = <1000>;
						full-duplex;
					};
				};

				port@1 {
					reg = <1>;
					label = "sw0p1";
					phy-handle = <&switch0phy1>;
				};
				port@2 {
					reg = <2>;
					label = "sw0p2";
					phy-handle = <&switch0phy2>;
				};
				port@3 {
					reg = <3>;
					label = "sw0p3";
					phy-handle = <&switch0phy3>;
				};
				port@4 {
					reg = <4>;
					label = "sw0p4";
					phy-handle = <&switch0phy4>;
				};
			};

			mdio {
				#address-cells = <1>;
				#size-cells = <0>;

				switch0phy1: switch0phy1@11 {
					reg = <0x11>;
				};
				switch0phy2: switch0phy2@12 {
					reg = <0x12>;
				};
				switch0phy3: switch0phy3@13 {
					reg = <0x13>;
				};
				switch0phy4: switch0phy4@14 {
					reg = <0x14>;
				};
			};
		};
	};
};

&gpc {
	/* use ldo-enable, u-boot will check it and configure */
	fsl,ldo-bypass = <1>;
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	pbc_pcb_sensor: tc74@4d {
		compatible = "tc74";
		reg = <0x4d>;
	};

	eeprom: eeprom@50 {
		compatible = "microchip,24c16";
		reg = <0x50>;
		pagesize = <16>;
	};

	/* tilt sensor */
	lsm303agr_acc@19 { /* Accelerometer */
		compatible = "st,lsm303agr-accel";
		reg = <0x19>;
	};
	lsm303agr_mag@1e { /* Magnetometer */
		compatible = "st,lsm303agr-magn";
		reg = <0x1e>;
		st,drdy-int-pin = <1>;
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	/* power module interface */
	pca9539_gpio_ioexp: gpio-controller@74 {
		compatible = "nxp,pca9539";
		reg = <0x74>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-parent = <&gpio1>;
		interrupts = <0 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&gpio3 20 GPIO_ACTIVE_LOW>;
		vcc-supply = <&reg_p3v3>;
	};
};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";

	/*
	 * PMIC is pre-programmed and so the "regulator-always-on" is
	 * actually the regulator can not be configured or changed. So,
	 * use this variant across this sub-tree
	 */
	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};
				regulator-state-mem {
					regulator-on-in-suspend;
				};
				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
				regulator-boot-on;
				regulator-always-on;
				vin-supply = <&reg_p4v5>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};
		};
	};
};

&pwm1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm1>;
	status = "okay";
};

&pwm2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm2>;
	status = "okay";
};

&reg_arm {
	vin-supply = <&sw2_reg>;
	regulator-allow-bypass;
};

&reg_soc {
	vin-supply = <&sw1c_reg>;
	regulator-allow-bypass;
};

&reg_pu {
	vin-supply = <&sw1c_reg>;
	regulator-allow-bypass;
};

&uart1 { /* console */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&uart2 { /* RS485 */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	rts-gpios = <&gpio3 25 GPIO_ACTIVE_HIGH>; /* RS485_DE */
	/* RS485_REn is set to low in uboot i.e we are always receive ready */
	linux,rs485-enabled-at-boot-time;
	rs485-rx-during-tx;
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usbotg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg>;
	disable-over-current;
	srp-disable;
	hnp-disable;
	adp-disable;
	dr_mode = "otg";
	status = "okay";
};

&usdhc3 {       /* eMMC */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3>;
	bus-width = <8>;
	non-removable;
	no-1-8-v;
	keep-power-in-suspend;
	status = "okay";
};

&ecspi1 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio4 9 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1>;
	status = "okay";
	/* appears no driver for ADS114S08B RTD */
	spidev0: spi@0 {
		/* just to disable boot warning from spidev driver */
		compatible = "rohm,dh2228fv";
		reg = <0>;
		/* 10Mhz, not sure if this is correct for ADS114 */
		spi-max-frequency = <10000000>;
	};
};
